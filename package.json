{
  "name": "@pivot-quant/institutional-pivot-engine",
  "version": "2.0.0",
  "description": "Institutional-grade pivot point analysis system for quantitative trading",
  "type": "module",
  "main": "src/index.js",
  "module": "src/index.js",
  "exports": {
    ".": "./src/index.js",
    "./core": "./src/core/QuantPivotEngine.js",
    "./math": "./src/math/MathematicalModels.js",
    "./validation": "./src/validation/ValidationFramework.js",
    "./monitoring": "./src/monitoring/PerformanceMonitor.js",
    "./config": "./src/config/ConfigurationManager.js"
  },
  "scripts": {
    "test": "jest --coverage --verbose",
    "test:watch": "jest --watch",
    "test:performance": "jest --testNamePattern=\"Performance\"",
    "test:stress": "jest --testNamePattern=\"Stress\"",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "build": "rollup -c",
    "build:prod": "NODE_ENV=production rollup -c",
    "docs": "jsdoc -c jsdoc.config.json",
    "benchmark": "node benchmarks/pivot-benchmark.js",
    "validate": "npm run lint && npm run test",
    "prepare": "npm run validate && npm run build",
    "start:dev": "node --experimental-modules examples/development-example.js",
    "start:prod": "node --experimental-modules examples/production-example.js",
    "dashboard:server": "node server/yahoo_proxy.js",
    "gamma:bridge": "bash server/run_gamma_bridge.sh",
    "event:writer": "bash server/run_event_writer.sh",
    "ml:init-db": "python3 scripts/init_db.py",
    "ml:log-events": "python3 scripts/run_logger.py --events data/events.jsonl",
    "ml:build-labels": "python3 scripts/build_labels.py --horizons 5 15 60 --incremental",
    "ml:export-csv": "python3 scripts/export_csv.py",
    "ml:export-parquet": "python3 scripts/export_parquet.py",
    "ml:duckdb-view": "python3 scripts/build_duckdb_view.py",
    "ml:cleanup-bars": "python3 scripts/cleanup_bar_data.py --prune",
    "ml:train-rf": "python3 scripts/train_rf.py",
    "ml:train-artifacts": "python3 scripts/train_rf_artifacts.py",
    "ml:backfill": "python3 scripts/backfill_events.py",
    "ml:server": "bash server/run_ml_server.sh",
    "start:all": "bash server/run_all.sh",
    "ml:migrate-bars": "python3 scripts/migrate_bar_data_pk.py"
  },
  "keywords": [
    "pivot-points",
    "quantitative-finance",
    "trading",
    "technical-analysis",
    "institutional",
    "financial-mathematics",
    "algorithmic-trading",
    "risk-management",
    "performance-analytics",
    "market-microstructure"
  ],
  "author": "PIVOT_QUANT Team",
  "license": "Proprietary",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "babel-jest": "^29.5.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.0",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "jsdoc": "^4.0.2",
    "prettier": "^2.8.8",
    "rollup": "^3.24.0",
    "rollup-plugin-filesize": "^10.0.0"
  },
  "dependencies": {
    "lightweight-charts": "^5.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "16"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended",
      "prettier"
    ],
    "plugins": [
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "repository": {
    "type": "git",
    "url": "internal"
  },
  "bugs": {
    "url": "internal"
  },
  "homepage": "internal"
}
